<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app id="WebApp_ID">
	<servlet>
	    <servlet-name>translate</servlet-name>
	    <servlet-class>test.servlet.TranslateServlet</servlet-class>
	    <load-on-startup>1</load-on-startup>
	</servlet>
	
	<servlet>
	    <servlet-name>tgl_translate</servlet-name>
	    <servlet-class>test.servlet.TGLTranslateServlet</servlet-class>
	    <load-on-startup>1</load-on-startup>
	</servlet>
	
    <servlet-mapping>
    	<servlet-name>translate</servlet-name>
    	<url-pattern>/translate</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
    	<servlet-name>tgl_translate</servlet-name>
    	<url-pattern>/tgl_translate</url-pattern>
    </servlet-mapping>
</web-app>
